{

  
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  
  "builds": [{
      "src": "auth_app/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
      }],
      "routes": [
          {
              "src": "/(.*)",
              "dest": "auth_app/wsgi.py"
          }
              ]

              }
